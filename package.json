    "intellisense",
    "completion",
    "hover",
    "lsp",
    "languageclient",
    "language-client",
    "language-server-protocol",
    "language server protocol"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danr/kakoune-languageclient.git"
  },
  "bugs": {
    "url": "https://github.com/danr/kakoune-languageclient/issues"
